BEGIN ;
    PROCEDURE PRINTSEVEN ;
        VARIABLE X 1 ;
        X := 7 ;
        WRITE X ;
    ENDPROCEDURE ;
    PROCEDURE RUN ;
        VARIABLE X 1 ;
        VARIABLE Y 1 ;
        X := 1 ;
        Y := 2 ;
        X := X + Y ;
        WRITE X ;
        WRITE Y ;

        PRINTSEVEN ;
    ENDPROCEDURE ;
END ;