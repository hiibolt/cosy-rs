BEGIN;
    {Main procedure}
    PROCEDURE RUN;
        VARIABLE (RE 1 2 3 4) X;
        LOOP I 1 1;
            LOOP J 1 2;
                LOOP K 1 3;
                    LOOP L 1 4;
                        X[I, J, K, L] := I + J + K + L;
                    ENDLOOP;
                ENDLOOP;
            ENDLOOP;
        ENDLOOP ;
        WRITE 6 "X: " X[1, 2, 3, 4];
    ENDPROCEDURE;
    
    {Call the main procedure}
    RUN;
END;