BEGIN ;
    PROCEDURE TESTSTRINGS ;
        VARIABLE MSG 1 ;
        VARIABLE GREETING 1 ;
        VARIABLE NUMBER 1 ;
        
        MSG := "Hello, World!" ;
        GREETING := "Hi" ;
        NUMBER := 42 ;
        
        WRITE "String literal: " MSG ;
        WRITE "Concatenation: " GREETING + " there!" ;
        WRITE "String + number: " "Count: " + NUMBER ;
        WRITE "Mixed expression: " "Result is " + (5 + 3) ;
    ENDPROCEDURE ;

    PROCEDURE RUN ;
        TESTSTRINGS ;
    ENDPROCEDURE ;
END ;
